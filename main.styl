.menuFlex
	background-color #070602
	position fixed
	padding padding/2
	display flex
	flex-direction row
	justify-content space-around
	width 100%
	top 0
	padding 0 20px
	height 57px
	//overflow hidden
	.imgItem
		margin-top 3px
		margin-right 40px
		padding-left 0
		margin-left 0
		width 10%
		height 10%
	span
		top 10px
		margin-top 1px
		background-color #e48f13
		padding 20px
	//border 1px solid black
	.menuItem
		font-size 16pt
		text-align: center;
		top 10px
		//
		// border 1px solid black
		justify-content space-around
		vertical-align center
		line-height: 2.5em;
		color white
		a
			text-decoration none
			font-weight: bold
			color white
		a:hover
			background-color #c4beba
			padding 5px
			transition 0.3s
		a:not(:hover)
			transition 0.5s
@media (max-width: 520px)
	.menuFlex
		span
			line-height 1em
		.menuItem
			line-height 2em
@media (max-width: 700px)
	.centralBlock
		.galleryGrid
			grid-template-columns 1fr 1fr
			grid-template-rows auto auto auto auto  auto auto
			grid-template-areas "description description""A B""C D""E F""G H""I ."
		.galleryBIGGrid
			grid-template-columns 1fr 1fr
			grid-template-rows auto auto auto auto  auto auto auto
			grid-template-areas "upu upu""A B" "C D" "E F" "G H" "I description"
		.galleryDOUBLEGrid
			grid-template-columns 1fr 1fr
			grid-template-rows auto auto auto auto  auto auto auto auto
			grid-template-areas "upu upu""A B" "C D" "E F" "G H" "I ." "uvu uvu" "AA BB" "CC ."
