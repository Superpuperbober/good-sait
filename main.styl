body
	top 100px
	color #212529
	margin 0
	padding 0
	line-height 1.4
	font-family -apple-system,BlinkMacSystemFont,Open Sans,Roboto,Helvetica Neue,Helvetica,sans-serif
	overflow-x hidden
	position relative
	display block
	box-sizing border-box
	background #f2f2f3

.centralBlock
	margin auto
	width 90%
	min-height 700px
	min-width 100px
	a
		color black
		text-decoration none
	a:hover
		text-decoration orange
		color orange
	.galleryGrid
		display grid
		grid-template-columns  repeat(10,1fr)
		grid-template-rows 1fr
		column-gap 20px
		justify-items stretch
		grid-template-areas "description A B C D E F G H I"
		height auto
		margin-bottom 4vh
		background #ffffff
		padding 10px
		align-items center
		vertical-align center
		//padding-bottom 30px
		img
			height 100%
			width 100%
			margin-top 15px
			padding-bottom 15px
			padding-right 5px
			padding-left 5px
		//border thin solid black
		img:hover
			width 110%
			height 110%
			transition 0.5s
			margin-top 0
			padding-bottom 10px
		img:not(:hover)
			transition 0.5s
		.description
			grid-area description
			p
				font-size 12pt
				text-align justify
			.top
				p
					font-size 15pt
					font-weight bold
				p:hover
					text-decoration #c4beba
					color #c4beba
				p:not(:hover)
					transition 0.5s
		.img1
			grid-area A
		.img2
			grid-area B
		.img3
			grid-area C
		.img4
			grid-area D
		.img5
			grid-area E
		.img6
			grid-area F
		.img7
			grid-area G
		.img8
			grid-area H
		.img9
			grid-area I
			p
				font-size 12pt
				text-align justify
			.top
				p
					font-size 15pt
					font-weight bold
				p:hover
					text-decoration #c4beba
					color #c4beba
				p:not(:hover)
					transition 0.5s
